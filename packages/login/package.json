{
  "name": "@oneaccount/login",
  "private": true,
  "version": "0.2.0-alpha.0",
  "description": "The login Node/React app",
  "scripts": {
    "lint": "eslint .",
    "start": "bin/start",
    "start:dev": "NODE_ENV=development BABEL_ENV=node NODE_CONFIG_DIR=./src/config babel-watch ./src/index.js",
    "start:prod": "NODE_ENV=production NODE_CONFIG_DIR=./dist/config node ./dist/index.js",
    "test": "NODE_CONFIG_DIR=./src/config BABEL_ENV=test jest --coverage",
    "test:watch": "NODE_CONFIG_DIR=./src/config BABEL_ENV=test jest --watch",
    "build:dev": "NODE_ENV=development npm run clean && webpack && npm run build-server",
    "build:prod": "NODE_ENV=production npm run clean && webpack && npm run build-server",
    "build-server": "BABEL_ENV=node babel ./src -d ./dist --ignore ./src/client,./src/vendor,test.js --copy-files",
    "clean": "rm -rf dist/",
    "snyk:test": "snyk test",
    "snyk:protect": "snyk protect",
    "snyk:wizard": "snyk wizard",
    "docker": "docker-compose up",
    "npm_token": "NPM_AUTH_TOKEN=$(grep 'nexus.ourtesco.com/repository/tesco-npm/' ~/.npmrc | cut -d '=' -f 2 | head -1)",
    "docker-build": "npm run npm_token -- docker-compose build",
    "docker-clean": "docker system prune",
    "docker-test": "npm run npm_token -- docker-compose -f docker-compose.test.yml build && docker-compose -f docker-compose.test.yml up -d app nginx; sleep 5; docker-compose -f docker-compose.test.yml up",
    "prepare": "npm run snyk:protect",
    "update-beans": "update-beans"
  },
  "dependencies": {
    "@beans/appbar": "^14.20.10",
    "@beans/breadcrumb": "^14.20.7",
    "@beans/button": "^14.20.8",
    "@beans/dropdown": "^14.20.7",
    "@beans/footer": "^14.20.8",
    "@beans/form-group": "^14.20.7",
    "@beans/foundation": "^14.20.7",
    "@beans/grid": "^14.20.7",
    "@beans/header": "^14.20.7",
    "@beans/helpers": "^14.20.5",
    "@beans/icon": "^14.20.7",
    "@beans/input": "^14.20.7",
    "@beans/input-group": "^14.20.7",
    "@beans/label": "^14.20.7",
    "@beans/link": "^14.20.7",
    "@beans/masthead": "^14.20.10",
    "@beans/notification": "^14.20.7",
    "@beans/primary-navigation": "^14.20.10",
    "@beans/tesco-modern": "^14.20.4",
    "@beans/theme": "^14.20.5",
    "@beans/typography": "^14.20.7",
    "@oneaccount/auth-utils": "0.1.0",
    "@oneaccount/node-logger-whitelists": "^1.4.2",
    "@oneaccount/react-foundations": "^0.24.0",
    "@testing-library/react": "^8.0.7",
    "@web-foundations/express-identity-validate": "^1.1.1",
    "@web-foundations/express-locale-middleware": "^1.0.5",
    "@web-foundations/express-traceid": "^0.2.0",
    "@web-foundations/express-user-segmentation": "^0.1.1",
    "@web-foundations/logger": "^1.4.0",
    "@web-foundations/service-identity": "^3.21.0",
    "accept-language": "^3.0.18",
    "aes256": "^1.0.4",
    "ajv-errors": "^1.0.1",
    "babel-eslint": "^10.0.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "config": "^3.2.2",
    "connected-react-router": "^6.5.2",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.9",
    "csurf": "^1.10.0",
    "express": "^4.17.1",
    "formik": "^1.5.8",
    "helmet": "^3.20.0",
    "history": "^4.9.0",
    "hoist-non-react-statics": "^3.3.0",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "memoize-one": "^5.0.5",
    "mobile-detect": "^1.4.3",
    "node-polyglot": "^2.3.1",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-testing-library": "^8.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "serialize-javascript": "^1.7.0",
    "styled-components": "^4.3.2",
    "uuid-validate": "0.0.3",
    "winston": "^2.3.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@beans/cli": "^14.20.4",
    "@web-foundations/eslint-config-base": "^1.1.3",
    "@web-foundations/eslint-config-react": "^1.1.3",
    "assets-webpack-plugin": "^3.9.10",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-watch": "^7.0.0",
    "concurrently": "^4.1.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "eslint": "~6.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "fetch-mock": "^7.3.9",
    "file-loader": "^4.1.0",
    "husky": "^3.0.1",
    "imagemin-lint-staged": "^0.4.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-immutable-matchers": "^2.0.1",
    "jsdom": "^15.1.1",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "sass-loader": "^7.1.0",
    "script-loader": "^0.7.2",
    "snapshot-diff": "^0.5.2",
    "snyk": "^1.202.0",
    "stats-webpack-plugin": "^0.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "Firefox >= 20",
    "Chrome >= 25",
    "Safari >= 7",
    "ie >= 8",
    "android > 3",
    "samsung > 2",
    "Last 5 versions"
  ],
  "snyk": true,
  "jest": {
    "setupFiles": [
      "./test/jest.config.js"
    ],
    "setupFilesAfterEnv": [
      "./test/jest.framework.setup.js"
    ],
    "testMatch": [
      "**/src/**/*test.js"
    ],
    "testPathIgnorePatterns": [
      "/src/config/",
      "/integration/config/",
      "/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  }
}
