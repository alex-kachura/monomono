{
  "env": {
    "test": {
      "plugins": ["@babel/plugin-proposal-class-properties"],
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/react"
      ]
    },
    "browser": {
      "plugins": ["@babel/plugin-proposal-class-properties"],
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": [
                "ie 9"
              ]
            }
          }
        ],
        "@babel/react"
      ]
    },
    "node": {
      "plugins": ["@babel/plugin-proposal-class-properties"],
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/react"
      ]
    }
  }
}
